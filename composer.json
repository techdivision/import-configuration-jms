{
    "name": "techdivision/import-configuration-jms",
    "description": "Library providing a JMS based configuration implementation for Pacemaker",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^2.2.2",
        "doctrine/annotations": "^1.14.0|^2.0.1",
        "jms/serializer": "^3.30.0",
        "jean85/pretty-package-versions": "^2.0.6",
        "symfony/expression-language": "~6.4.8",
        "techdivision/import": "^18.0.0",
        "techdivision/import-dbal": "^2.0.0"
    },
    "require-dev": {
        "doctrine/dbal": "^4.0.4",
        "pdepend/pdepend": "^2.16.2",
        "phpmd/phpmd": "^2.15.0",
        "phpunit/phpunit": "^11.2.5",
        "sebastian/phpcpd": "^2.0.1",
        "squizlabs/php_codesniffer": "^3.10.1",
        "consolidation/robo": "^4.0.2",
        "mikey179/vfsstream": "~1.6.11",
        "symfony/http-kernel": "~4.4.51"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TechDivision\\Import\\Configuration\\Jms\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechDivision\\Import\\Configuration\\Jms\\": [
                "tests/unit"
            ]
        }
    }
}
